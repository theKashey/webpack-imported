{
  "name": "webpack-imported",
  "version": "1.2.2",
  "description": "You chunks importing buddy",
  "main": "dist/es5/index.js",
  "module:es2019": "dist/es2019/index.js",
  "module": "dist/es2015/index.js",
  "types": "dist/es5/index.d.ts",
  "scripts": {
    "dev": "lib-builder dev",
    "test": "jest",
    "test:ci": "jest --runInBand --coverage",
    "build": "lib-builder build && yarn size:report",
    "release": "yarn build && yarn test",
    "size": "npx size-limit",
    "size:report": "npx size-limit --json > .size.json",
    "lint": "lib-builder lint",
    "format": "lib-builder format",
    "update": "lib-builder update",
    "prepublish": "yarn build && yarn changelog",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:rewrite": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "sideEffects": false,
  "repository": "https://github.com/theKashey/webpack-imported/",
  "author": "theKashey <thekashey@gmail.com>",
  "license": "MIT",
  "resolutions": {
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.2",
    "size-limit": "^11.0.2",
    "@theuiteam/lib-builder": "^0.3.0",
    "@types/lodash": "^4.14.138",
    "webpack": "^5.28.5",
    "@types/node": "^18.0.0"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "css",
    "react"
  ],
  "keywords": [
    "webpack",
    "stat.json",
    "chunks",
    "codesplitting"
  ],
  "dependencies": {
    "kashe": "^1.0.3",
    "tslib": "^2.0.1"
  },
  "peerDependencies": {
    "@types/react": "^16.8.0 || ^17.0.0",
    "react": "^16.8.0 || ^17.0.0"
  },
  "size-limit": [
    {
      "path": [
        "dist/es2015/react/index.js"
      ],
      "ignore": [
        "tslib",
        "react"
      ],
      "limit": "3 KB"
    }
  ],
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 120,
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  }
}
